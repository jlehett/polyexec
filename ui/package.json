{
  "name": "electron-react-vite",
  "productName": "electron-react-vite",
  "description": "An amazing app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "electron.cjs",
  "scripts": {
    "start-gui": "node start-gui.js",
    "build": "cross-env NODE_ENV=production vite build",
    "preview": "vite preview"
  },
  "build": {
    "appId": "electron-react-vite",
    "mac": {
      "icon": "your-icon"
    },
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "your-icon"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "your-icon",
      "uninstallerIcon": "your-icon",
      "uninstallDisplayName": "electron-react-vite"
    },
    "directories": {
      "output": "out"
    },
    "files": [
      "dist/**/*",
      "electron.cjs",
      "electron/**"
    ]
  },
  "dependencies": {
    "electron": "^24.3.1",
    "vite": "^4.3.2"
  },
  "devDependencies": {
    "@psionic/emit": "^0.0.2",
    "@psionic/emit-react": "^0.0.3",
    "@psionic/ui": "^1.10.0",
    "framer-motion": "^11.0.20",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3",
    "sass": "^1.71.1",
    "vite-plugin-svgr": "^4.2.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^4.0.0",
    "concurrently": "^8.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.38.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4"
  }
}
